{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)/?",
      "destination": "https://simple-iot-dashboard.vercel.app/$1",
      "type": 301
    }
  ]
}
